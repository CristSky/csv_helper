{
  "name": "CSV Helper",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "CSV Helper",
    "default_popup": "src/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "activeTab",
    "https://*/*"
  ]
}